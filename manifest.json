{
  "manifest_version": 3,
  "name": "Password Strength Checker",
  "version": "1.0",
  "description": "Checks password strength in real-time.",
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "lock.png"
  }
}
